{
  "name": "vue-tokens-presenters",
  "version": "1.0.0",
  "private": false,
  "main": "dist/vue-tokens-presenters.common.js",
  "web": "dist/bootstrap-vue.umd.js",
  "unpkg": "dist/vue-tokens-presenters.umd.min.js",
  "source": "src/index.ts",
  "style": "dist/vue-tokens-presenters.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/11joselu/vue-tokens-presenters.git"
  },
  "keywords": [
    "Presenter",
    "Storybook"
  ],
  "author": "Jose Cabrera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11joselu/vue-tokens-presenters/issues"
  },
  "homepage": "https://github.com/11joselu/vue-tokens-presenters.git#readme",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target=lib",
    "lint": "vue-cli-service lint",
    "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "storybook:export": "vue-cli-service storybook:build -c config/storybook",
    "test": "vue-cli-service test:unit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": ">=5.3.0",
    "@storybook/addon-docs": ">=5.3.0",
    "@storybook/vue": ">=5.3.0",
    "@testing-library/vue": "^5.0.4",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-storybook": "~1.3.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
